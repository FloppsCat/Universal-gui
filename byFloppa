-- Scripts:

local function YCEI_fake_script() -- SpeedTextBox.LocalScript 
	local script = Instance.new('LocalScript', SpeedTextBox)

	local textBox = script.Parent
	
	local function adjustWalkSpeed()
		local walkSpeed = tonumber(textBox.Text)
		if walkSpeed then
			local player = game.Players.LocalPlayer
			if player and player.Character then
				player.Character.Humanoid.WalkSpeed = walkSpeed
				print(player.Name .."'s WalkSpeed has been changed to " ..walkSpeed)
			end
		else
			textBox.Text = "Please enter a valid WalkSpeed."
		end
	end
	
	textBox.FocusLost:Connect(adjustWalkSpeed)
	
end
coroutine.wrap(YCEI_fake_script)()
local function NRNOLHD_fake_script() -- JumpTextBox.LocalScript 
	local script = Instance.new('LocalScript', JumpTextBox)

	local textBox = script.Parent
	
	local function adjustJumpPower()
		local JumpPower = tonumber(textBox.Text)
		if JumpPower then
			local player = game.Players.LocalPlayer
			if player and player.Character then
				player.Character.Humanoid.JumpPower = JumpPower
				print(player.Name .."'s WalkSpeed has been changed to " ..JumpPower)
			end
		else
			textBox.Text = "Please enter a valid WalkSpeed."
		end
	end
	
	textBox.FocusLost:Connect(adjustJumpPower)
end
coroutine.wrap(NRNOLHD_fake_script)()
